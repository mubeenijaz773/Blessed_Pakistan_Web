{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/app/api/Product/route.jsx",
    "src/app/Loader/page.jsx",
    "src/app/api/Fetch_Agency/route.js",
    "src/app/Components/BrowseProperties/page.jsx",
    "src/app/Components/BrowseProperties/page.js",
    "build/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
